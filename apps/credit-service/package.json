  },
  "devDependencies": {
    "@types/amqplib": "^0.10.7",
    "@types/node": "^20",
    "nodemon": "^3.1.10",
    "prisma": "^5.22.0",
    "ts-node": "10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.3"
